{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express TypeScript API for DataCloak Sentiment Workbench - Production ready with 82.1% test coverage",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:monitor": "node scripts/test-with-monitoring.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage:enforce": "node scripts/enforce-coverage.js",
    "coverage:check": "jest --coverage --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:integration:all": "node scripts/integration-test-coordinator.js",
    "test:integration:coordinate": "node scripts/integration-test-coordinator.js",
    "test:selective": "node scripts/selective-test-runner.js",
    "test:critical": "node scripts/selective-test-runner.js critical",
    "test:redis": "node scripts/selective-test-runner.js redis",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:regression": "jest src/tests/regression --testTimeout=60000 --verbose",
    "test:fast": "jest --config jest.config.fast.js",
    "test:quick": "QUICK_TEST=true jest --testTimeout=3000 --maxWorkers=4",
    "test:parallel": "jest --maxWorkers=50%",
    "test:unit": "jest **/__tests__/**/*.ts --testTimeout=3000 --maxWorkers=4",
    "test:optimize": "node scripts/optimize-test-timeouts.js",
    "test:job-queue": "ts-node scripts/test-job-queue.ts",
    "test:large-files": "ts-node src/tests/performance/large-file-test.ts",
    "test:performance": "jest src/tests/performance --testTimeout=60000 --verbose",
    "test:e2e": "jest src/tests/e2e --testTimeout=120000 --verbose",
    "test:load": "jest src/tests/load --testTimeout=120000 --verbose",
    "test:performance:cache": "jest src/tests/cache-performance.test.ts --testTimeout=30000",
    "test:performance:jobs": "jest src/tests/performance/job-queue-performance.test.ts --testTimeout=60000",
    "test:performance:api": "jest src/tests/performance/api-performance.test.ts --testTimeout=60000",
    "test:performance:cache-load": "jest src/tests/performance/cache-load-testing.test.ts --testTimeout=60000",
    "test:performance:persistence": "jest src/tests/performance/job-queue-persistence.test.ts --testTimeout=60000",
    "test:config-reload": "jest src/tests/config-hot-reload.test.ts --testTimeout=15000",
    "test:rate-limit": "ts-node src/tests/performance/rate-limit-test.ts",
    "test:datacloak-ffi": "ts-node src/tests/integration/datacloak-ffi-test.ts",
    "secrets:init": "ts-node scripts/manage-secrets.ts init",
    "secrets:list": "ts-node scripts/manage-secrets.ts list",
    "secrets:validate": "ts-node scripts/manage-secrets.ts validate",
    "secrets:generate": "ts-node scripts/manage-secrets.ts generate",
    "secrets:rotate": "ts-node scripts/manage-secrets.ts rotate",
    "secrets:rotation-status": "ts-node scripts/manage-secrets.ts rotation-status",
    "secrets:export-audit": "ts-node scripts/manage-secrets.ts export-audit",
    "secrets:interactive": "ts-node scripts/manage-secrets.ts interactive",
    "test:datacloak-integration": "ts-node src/tests/integration/datacloak-integration-test.ts",
    "test:datacloak-rate-limit": "ts-node src/tests/integration/datacloak-rate-limit-test.ts",
    "test:datacloak-enhanced": "ts-node src/tests/integration/enhanced-datacloak-test.ts",
    "test:risk-assessment": "ts-node src/tests/integration/risk-assessment-test.ts",
    "test:sse-progress": "ts-node src/tests/integration/sse-progress-test.ts",
    "test:websocket-load": "ts-node src/tests/integration/websocket-load-test.ts",
    "test:websocket-concurrent": "ts-node src/tests/integration/websocket-concurrent-test.ts",
    "test:websocket-stress": "ts-node src/tests/integration/websocket-stress-test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "migrate": "ts-node src/database/migration-cli.ts",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "express",
    "typescript",
    "sentiment-analysis",
    "api",
    "sqlite",
    "duckdb",
    "testing"
  ],
  "author": "DataCloak Team",
  "license": "MIT",
  "dependencies": {
    "@dsw/security": "file:../security",
    "@types/node-cron": "^3.0.11",
    "@types/validator": "^13.15.2",
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.10.0",
    "chokidar": "^3.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "duckdb": "^1.3.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "koffi": "^2.8.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0",
    "papaparse": "^5.5.3",
    "tail": "^2.2.6",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.1",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
