{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express TypeScript API for DataCloak Sentiment Workbench - Production ready with 82.1% test coverage",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:job-queue": "ts-node scripts/test-job-queue.ts",
    "test:large-files": "ts-node scripts/test-large-files.ts",
    "test:performance": "ts-node src/tests/performance/large-file-test.ts",
    "test:rate-limit": "ts-node src/tests/performance/rate-limit-test.ts",
    "test:datacloak-ffi": "ts-node src/tests/integration/datacloak-ffi-test.ts",
    "test:sse-progress": "ts-node src/tests/integration/sse-progress-test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": ["express", "typescript", "sentiment-analysis", "api", "sqlite", "duckdb", "testing"],
  "author": "DataCloak Team",
  "license": "MIT",
  "dependencies": {
    "@dsw/security": "file:../security",
    "better-sqlite3": "^11.10.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "duckdb": "^1.3.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
